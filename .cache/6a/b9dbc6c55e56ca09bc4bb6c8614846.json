ve{\r\n  height: 100px;\r\n  width: 100px;\r\n  border-radius: 50%;\r\n  background-color: white;\r\n  opacity: 0.1;\r\n  display: block;\r\n  position: fixed;\r\n  z-index: -1;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-weight: bold;\r\n  letter-spacing: 2px;\r\n  color: #2e005f;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.mouse-move-hidden{\r\n  opacity: 0;\r\n}\r\n\r\nnav{\r\n  position: absolute;\r\n  top: 0;\r\n  max-width: 1200px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: yellow;\r\n  font-size: 3rem;\r\n  margin: 0 auto;\r\n  letter-spacing: 5px;\r\n  /* font-family: 'Press Start 2P', cursive; */\r\n  font-family: 'Fredericka the Great', cursive;\r\n  padding: 20px;\r\n  height: 80px;\r\n}\r\n\r\nnav .title{\r\n    position: relative;\r\n}\r\n\r\nnav .title::after{\r\n    position: absolute;\r\n    height: 5px;\r\n    border-radius: 20px;\r\n    width: 0;\r\n    content: '';\r\n    background-color: yellow;\r\n    left: 0;\r\n    bottom: 0;\r\n}\r\n\r\n.game {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  margin-top: 80px;\r\n}\r\n\r\n.text {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 20px;\r\n  width: 860px;\r\n}\r\n\r\n.word-container {\r\n  padding: 10px 5px;\r\n  min-width: 20px;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.letter-word-container {\r\n  position: relative;\r\n  font-size: 2rem;\r\n  margin: 5px;\r\n  width: 0;\r\n  padding: 5px 0;\r\n  text-align: center;\r\n  color: white;\r\n  overflow: hidden;\r\n}\r\n\r\n.apparition{\r\n  animation: apparition 0.3s ease-out forwards;\r\n}\r\n\r\n@keyframes apparition {\r\n  from{opacity: 0; width: 0;}\r\n  to{opacity: 1; width: 30px;}\r\n}\r\n\r\n.letter-word {\r\n  transition: all 1s ease-in-out;\r\n  display: block;\r\n}\r\n\r\n.letter-word-hidden {\r\n  transform: translateY(100%) rotateY(720deg);\r\n  transform-origin: center;\r\n}\r\n\r\n.letter-word-container::after {\r\n  min-width: 20px;\r\n  position: absolute;\r\n  border-radius: 20px;\r\n  width: 100%;\r\n  height: 5px;\r\n  left: 0;\r\n  bottom: 0;\r\n  content: \"\";\r\n  background-color: yellow;\r\n}\r\n\r\n.messages{\r\n    font-size: 1.5rem;\r\n    color: white;\r\n    text-align: center;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.lives, .msg-end {\r\n  padding: 20px 0;\r\n}\r\n\r\n.lives{\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  letter-spacing: 2px;\r\n}\r\n\r\n.lives-sliders-container{\r\n  width: 20px;\r\n  height: 30px;\r\n  display: inline-block;\r\n  overflow: hidden;\r\n}\r\n\r\n.lives-sliders{\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  transition: all 1s ease-in-out;\r\n}\r\n\r\n.lives-sliders span{\r\n  height: 30px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.msg-end{\r\n    font-size: 2rem;\r\n    color: yellow;\r\n    transform: scale(0);\r\n    padding: 0 0 10px 0;\r\n    display: none;\r\n}\r\n\r\n.apears-msg-end{\r\n    animation: appears 0.5s ease-out forwards;\r\n    /* keyframes in hangman.css */\r\n}\r\n\r\n.reset{\r\n  font-size: 1.5rem;\r\n  padding: 5px 20px;\r\n  border: 1px solid white;\r\n  color: white;\r\n  background-color: transparent;\r\n  border-radius: 10px;\r\n  position: relative;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  margin-top: 10px;\r\n}\r\n\r\n.reset::after{\r\n  width: 100%;\r\n  height: 100%;\r\n  content: '';\r\n  position: absolute;\r\n  left: -100%;\r\n  bottom: -100%;\r\n  background-color: white;\r\n  transition: all 0.3s ease-out;\r\n  z-index: -1;\r\n  opacity: 0;\r\n}\r\n\r\n.reset:hover::after{\r\n  left: 0;\r\n  bottom: 0;\r\n  opacity: 1;\r\n}\r\n\r\n.reset:hover{\r\n  transition: all 0.3s ease-out;\r\n  color: #2e005f;\r\n}\r\n\r\n.keyboard {\r\n  padding: 5px;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  position: relative; /* for z-index at end of game */\r\n}\r\n\r\n.letter-keyboard {\r\n  background-color: transparent;\r\n  font-size: 2rem;\r\n  border: 2px solid white;\r\n  padding: 5px 10px;\r\n  margin: 5px;\r\n  width: 50px;\r\n  text-transform: uppercase;\r\n  border-radius: 50%;\r\n  color: white;\r\n  cursor: pointer;\r\n  transition: all 0.2s ease-out;\r\n  transform: scale(0);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.letter-keyboard-scaling{\r\n  transform: scale(1);\r\n}\r\n\r\n.letter-keyboard:hover {\r\n  background-color: white;\r\n  color: #2e005f;\r\n  transform: scale(1.2);\r\n  box-shadow: 0px 5px 10px rgb(0, 0, 0, 0.5);\r\n}\r\n\r\n.not-found {\r\n  color: gray;\r\n  border-color: gray;\r\n}\r\n\r\n.found {\r\n  color: yellow;\r\n  border-color: yellow;\r\n}\r\n\r\n.not-found:not(.nohover):hover, .found:not(.nohover):hover{\r\n  background-color: transparent;\r\n  transform: scale(1);\r\n  box-shadow: unset;\r\n  cursor: auto;\r\n}\r\n\r\n.not-found:not(.nohover):hover{\r\n  color: gray;\r\n}\r\n\r\n.found:not(.nohover):hover{\r\n  color: yellow;\r\n}\r\n\r\n@media only screen and (max-width: 1200px){\r\n  .game{\r\n    width: 100%;\r\n  }\r\n}\r\n@media only screen and (max-width: 1080px) and (orientation: portrait){\r\n  .game{\r\n    flex-direction: column;\r\n    width: 100%;\r\n  }\r\n}\r\n@media only screen and (max-width: 1080px){\r\n  .text{\r\n    width: 100vw;\r\n    display: inline-block;\r\n  }\r\n  .title{\r\n    font-size: 3rem;\r\n  }\r\n  .letter-keyboard{\r\n    font-size: 1.5rem;\r\n    width: 30px;\r\n    padding: 5px 20px;\r\n  }\r\n}\r\n@media only screen and (orientation: landscape){\r\n  .game{\r\n    flex-direction: row;\r\n  }\r\n}\r\n@media only screen and (orientation: landscape) and (max-width: 650px){\r\n  .game{\r\n    flex-direction: column;\r\n  }\r\n}"},"lineCount":343}},"error":null,"hash":"f8f2a26d1db7bf1f3844c270151b67ab","cacheData":{"env":{}}}